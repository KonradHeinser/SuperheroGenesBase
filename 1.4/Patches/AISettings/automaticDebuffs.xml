<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="SuperHeroGenesBase.PatchOperationSetting">
        <setting>automaticDebuffs</setting>
        <active Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThinkTreeDef[defName="SHG_SuperAI"]/thinkRoot/subNodes/li/subNodes/li[1]/subNodes/li[6]/subNodes</xpath>
                    <value>
                        <li Class="ThinkNode_ConditionalHasAbility">
                            <ability>SHG_CommandAnimal</ability>
                            <subNodes>
                                <li Class="SuperHeroGenesBase.JobGiver_AICastBerserkAbility">
                                    <ability>SHG_CommandAnimal</ability>
                                </li>
                            </subNodes>
                        </li>
                        <li Class="ThinkNode_ConditionalHasAbility">
                            <ability>AnimalWarcall</ability>
                            <subNodes>
                                <li Class="SuperHeroGenesBase.JobGiver_AICastBerserkAbility">
                                    <ability>AnimalWarcall</ability>
                                </li>
                            </subNodes>
                        </li>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThinkTreeDef[defName="SHG_SuperAI"]/thinkRoot/subNodes/li/subNodes/li[1]/subNodes/li[2]/subNodes</xpath>
                    <value>
                        <li Class="ThinkNodeConditional_HashIntervalTick">
                            <interval>3</interval>
                            <subNodes>
                                <li Class="SuperHeroGenesBase.ThinkNode_ConditionalHasTargetNoFriendlyFire">
                                    <ability>SHG_Psionist_IcePlanet</ability>
                                    <minEnemies>5</minEnemies>
                                    <subNodes>
                                        <li Class="SuperHeroGenesBase.JobGiver_AICastAnyOfAbilityOnEnemyTarget">
                                            <abilities>
                                                <li>SHG_Psionist_IcePlanet</li>
                                                <li>SHG_Psionist_Eldritch</li>
                                            </abilities>
                                        </li>
                                    </subNodes>
                                </li>
                                <li Class="SuperHeroGenesBase.ThinkNode_ConditionalHasTargetNoFriendlyFire">
                                    <ability>SHG_Psionist_Psychedelic</ability>
                                    <minEnemies>3</minEnemies>
                                    <subNodes>
                                        <li Class="SuperHeroGenesBase.JobGiver_AICastAnyOfAbilityOnEnemyTarget">
                                            <abilities>
                                                <li>SHG_Psionist_Psychedelic</li>
                                                <li>SHG_Hemomancer_BloodAndDarkness</li>
                                            </abilities>
                                        </li>
                                    </subNodes>
                                </li>
                                <li Class="SuperHeroGenesBase.JobGiver_AICastAnyOfAbilityOnEnemyTarget">
                                    <abilities>
                                        <li>SHG_Psionist_InternalDebate</li>
                                        <li>SHG_Hemomancer_StealPerceptiveness</li>
                                        <li>SHG_Hemomancer_StealDefense</li>
                                        <li>SHG_Hemomancer_StealStrength</li>
                                        <li>SHG_Hemomancer_MarkForDeath</li>
                                    </abilities>
                                </li>
                                <li Class="SuperHeroGenesBase.ThinkNode_ConditionalUsingRangedWeapon">
                                    <invert>true</invert>
                                    <subNodes>
                                        <li Class="SuperHeroGenesBase.JobGiver_AICastAnyOfAbilityOnEnemyTarget"> <!--A last ditch ability to use even though the target may be immune-->
                                            <abilities>
                                                <li>SHG_Psionist_CompelledDuel</li>
                                            </abilities>
                                        </li>
                                    </subNodes>
                                </li>
                            </subNodes>
                        </li>
                    </value>
                </li>

                <li Class="SuperHeroGenesBase.PatchOperationSetting">
                    <setting>automaticDebuffsDrafted</setting>
                    <active Class="PatchOperationSequence">
                        <operations>
                            <li Class="PatchOperationAdd">
                                <xpath>Defs/ThinkTreeDef[defName="SHG_SuperAI"]/thinkRoot/subNodes/li/subNodes/li[2]/subNodes/li[5]/subNodes</xpath>
                                <value>
                                    <li Class="ThinkNode_ConditionalHasAbility">
                                        <ability>SHG_CommandAnimal</ability>
                                        <subNodes>
                                            <li Class="SuperHeroGenesBase.JobGiver_AICastBerserkAbility">
                                                <ability>SHG_CommandAnimal</ability>
                                            </li>
                                        </subNodes>
                                    </li>
                                    <li Class="ThinkNode_ConditionalHasAbility">
                                        <ability>AnimalWarcall</ability>
                                        <subNodes>
                                            <li Class="SuperHeroGenesBase.JobGiver_AICastBerserkAbility">
                                                <ability>AnimalWarcall</ability>
                                            </li>
                                        </subNodes>
                                    </li>
                                </value>
                            </li>
                            <li Class="PatchOperationAdd">
                                <xpath>Defs/ThinkTreeDef[defName="SHG_SuperAI"]/thinkRoot/subNodes/li/subNodes/li[2]/subNodes</xpath>
                                <value>
                                    <li Class="ThinkNodeConditional_HashIntervalTick">
                                        <interval>3</interval>
                                        <subNodes>
                                            <li Class="SuperHeroGenesBase.ThinkNode_ConditionalHasTargetNoFriendlyFire">
                                                <ability>SHG_Psionist_IcePlanet</ability>
                                                <minEnemies>5</minEnemies>
                                                <subNodes>
                                                    <li Class="SuperHeroGenesBase.JobGiver_AICastAnyOfAbilityOnEnemyTarget">
                                                        <abilities>
                                                            <li>SHG_Psionist_IcePlanet</li>
                                                            <li>SHG_Psionist_Eldritch</li>
                                                        </abilities>
                                                    </li>
                                                </subNodes>
                                            </li>
                                            <li Class="SuperHeroGenesBase.ThinkNode_ConditionalHasTargetNoFriendlyFire">
                                                <ability>SHG_Psionist_Psychedelic</ability>
                                                <minEnemies>3</minEnemies>
                                                <subNodes>
                                                    <li Class="SuperHeroGenesBase.JobGiver_AICastAnyOfAbilityOnEnemyTarget">
                                                        <abilities>
                                                            <li>SHG_Psionist_Psychedelic</li>
                                                            <li>SHG_Hemomancer_BloodAndDarkness</li>
                                                        </abilities>
                                                    </li>
                                                </subNodes>
                                            </li>
                                            <li Class="SuperHeroGenesBase.JobGiver_AICastAnyOfAbilityOnEnemyTarget">
                                                <abilities>
                                                    <li>SHG_Psionist_InternalDebate</li>
                                                    <li>SHG_Hemomancer_StealPerceptiveness</li>
                                                    <li>SHG_Hemomancer_StealDefense</li>
                                                    <li>SHG_Hemomancer_StealStrength</li>
                                                    <li>SHG_Hemomancer_MarkForDeath</li>
                                                </abilities>
                                            </li>
                                            <li Class="SuperHeroGenesBase.ThinkNode_ConditionalUsingRangedWeapon">
                                                <invert>true</invert>
                                                <subNodes>
                                                    <li Class="SuperHeroGenesBase.JobGiver_AICastAnyOfAbilityOnEnemyTarget"> <!--A last ditch ability to use even though the target may be immune-->
                                                        <abilities>
                                                            <li>SHG_Psionist_CompelledDuel</li>
                                                        </abilities>
                                                    </li>
                                                </subNodes>
                                            </li>
                                        </subNodes>
                                    </li>
                                </value>
                            </li>
                        </operations>
                    </active>
                </li>
            </operations>
        </active>
    </Operation>
</Patch>
