<?xml version="1.0" encoding="utf-8" ?>
<Defs><!--Calling it super sounds more than generous, but it's actually just uninventive!-->
    <ThinkTreeDef>
        <defName>SHG_SuperAI</defName>
        <insertTag>Humanlike_PostMentalState</insertTag>
        <insertPriority>100</insertPriority>
        <thinkRoot Class="ThinkNode_Priority">
            <subNodes>
                <li Class="ThinkNode_ConditionalColonist">
                    <subNodes>
                        <!--<xpath>Defs/ThinkTreeDef[defName="SHG_SuperAI"]/thinkRoot/subNodes/li/subNodes</xpath>-->
                        <li Class="ThinkNode_ConditionalCanDoConstantThinkTreeJobNow">
                            <subNodes>
                                <!--<xpath>Defs/ThinkTreeDef[defName="SHG_SuperAI"]/thinkRoot/subNodes/li/subNodes/li/subNodes</xpath>-->
                                <li Class="ThinkNode_ConditionalDrafted">
                                    <invert>true</invert>
                                    <subNodes>
                                        <!--<xpath>Defs/ThinkTreeDef[defName="SHG_SuperAI"]/thinkRoot/subNodes/li/subNodes/li/subNodes/li[1]/subNodes</xpath>-->
                                        <li Class="SuperHeroGenesBase.ThinkNode_ConditionalAnyActiveEnemyInMap">
                                            <invert>true</invert>
                                            <subNodes>
                                                <!--<xpath>Defs/ThinkTreeDef[defName="SHG_SuperAI"]/thinkRoot/subNodes/li/subNodes/li/subNodes/li[1]/subNodes/li[1]/subNodes</xpath>-->
                                            </subNodes>
                                        </li>
                                        <li Class="ThinkNode_ConditionalHasTarget">
                                            <subNodes>
                                                <!--<xpath>Defs/ThinkTreeDef[defName="SHG_SuperAI"]/thinkRoot/subNodes/li/subNodes/li/subNodes/li[1]/subNodes/li[2]/subNodes</xpath>-->
                                            </subNodes>
                                        </li>
                                        <li Class="ThinkNode_ConditionalShotRecently">
                                            <thresholdTicks>600</thresholdTicks>
                                            <subNodes>
                                                <!--<xpath>Defs/ThinkTreeDef[defName="SHG_SuperAI"]/thinkRoot/subNodes/li/subNodes/li/subNodes/li[1]/subNodes/li[3]/subNodes</xpath>-->
                                            </subNodes>
                                        </li>
                                        <li Class="SuperHeroGenesBase.ThinkNode_ConditionalTooManyNearbyThreats">
                                            <dangerRadius>20</dangerRadius>
                                            <minCount>5</minCount>
                                            <subNodes>
                                                <!--<xpath>Defs/ThinkTreeDef[defName="SHG_SuperAI"]/thinkRoot/subNodes/li/subNodes/li/subNodes/li[1]/subNodes/li[4]/subNodes</xpath>-->
                                            </subNodes>
                                        </li>
                                        <li Class="SuperHeroGenesBase.ThinkNode_ConditionalTooManyNearbyThreats">
                                            <subNodes>
                                                <!--<xpath>Defs/ThinkTreeDef[defName="SHG_SuperAI"]/thinkRoot/subNodes/li/subNodes/li/subNodes/li[1]/subNodes/li[5]/subNodes</xpath>-->
                                            </subNodes>
                                        </li>
                                        <li Class="SuperHeroGenesBase.ThinkNode_ConditionalAnyActiveEnemyInMap">
                                            <subNodes>
                                                <!--<xpath>Defs/ThinkTreeDef[defName="SHG_SuperAI"]/thinkRoot/subNodes/li/subNodes/li/subNodes/li[1]/subNodes/li[6]/subNodes</xpath>-->
                                            </subNodes>
                                        </li>
                                    </subNodes>
                                </li>
                                <li Class="ThinkNode_ConditionalDrafted">
                                    <subNodes>
                                        <!--<xpath>Defs/ThinkTreeDef[defName="SHG_SuperAI"]/thinkRoot/subNodes/li/subNodes/li/subNodes/li[2]/subNodes</xpath>-->
                                        <li Class="ThinkNode_ConditionalHasTarget">
                                            <subNodes>
                                                <!--<xpath>Defs/ThinkTreeDef[defName="SHG_SuperAI"]/thinkRoot/subNodes/li/subNodes/li/subNodes/li[2]/subNodes/li[1]/subNodes</xpath>-->
                                            </subNodes>
                                        </li>
                                        <li Class="ThinkNode_ConditionalShotRecently">
                                            <thresholdTicks>600</thresholdTicks>
                                            <subNodes>
                                                <!--<xpath>Defs/ThinkTreeDef[defName="SHG_SuperAI"]/thinkRoot/subNodes/li/subNodes/li/subNodes/li[2]/subNodes/li[2]/subNodes</xpath>-->
                                            </subNodes>
                                        </li>
                                        <li Class="SuperHeroGenesBase.ThinkNode_ConditionalTooManyNearbyThreats">
                                            <dangerRadius>20</dangerRadius>
                                            <minCount>5</minCount>
                                            <subNodes>
                                              <!--<xpath>Defs/ThinkTreeDef[defName="SHG_SuperAI"]/thinkRoot/subNodes/li/subNodes/li/subNodes/li[2]/subNodes/li[3]/subNodes</xpath>-->
                                            </subNodes>
                                        </li>
                                        <li Class="SuperHeroGenesBase.ThinkNode_ConditionalTooManyNearbyThreats">
                                            <subNodes>
                                              <!--<xpath>Defs/ThinkTreeDef[defName="SHG_SuperAI"]/thinkRoot/subNodes/li/subNodes/li/subNodes/li[2]/subNodes/li[4]/subNodes</xpath>-->
                                            </subNodes>
                                        </li>
                                        <li Class="SuperHeroGenesBase.ThinkNode_ConditionalAnyActiveEnemyInMap">
                                            <subNodes>
                                                <!--<xpath>Defs/ThinkTreeDef[defName="SHG_SuperAI"]/thinkRoot/subNodes/li/subNodes/li/subNodes/li[2]/subNodes/li[5]/subNodes</xpath>-->
                                            </subNodes>
                                        </li>
                                    </subNodes>
                                </li>
                            </subNodes>
                        </li>
                    </subNodes>
                </li>
            </subNodes>
        </thinkRoot>
    </ThinkTreeDef>
</Defs>