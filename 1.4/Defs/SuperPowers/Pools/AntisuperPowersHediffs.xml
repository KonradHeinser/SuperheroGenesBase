            <li>
                <minSeverity>0.001</minSeverity>
                <label>comatose</label>
                <capMods>
                    <li>
                        <capacity>Consciousness</capacity>
                        <setMax>0.1</setMax>
                    </li>
                </capMods>
            </li>
            <li>
