<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationAdd"> <!--Stops players from taking superclotting when they have SuperHealing because SuperHealing has superclotting as part of it-->
        <xpath>/Defs/GeneDef[defName="Superclotting"]</xpath>
        <value>
            <exclusionTags>
                <li>SuperHealings</li>
            </exclusionTags>
        </value>
    </Operation>
</Patch>
