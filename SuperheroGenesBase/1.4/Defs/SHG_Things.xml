        </graphicData>
        <plant>
            <growDays>7.50</growDays>
            <dieIfLeafless>true</dieIfLeafless>
            <drugForHarvestPurposes>True</drugForHarvestPurposes>
            <harvestedThingDef>SmokeleafLeaves</harvestedThingDef>
