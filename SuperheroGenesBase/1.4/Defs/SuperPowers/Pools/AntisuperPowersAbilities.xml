<?xml version="1.0" encoding="utf-8" ?>
<Defs>
            <!--Anti-super Durability-->
    <AbilityDef ParentName="SHGPowerAbilitiesBase">
        <defName>SHG_AntiPower_Durability_MinorAura</defName>
        <label>anti-durability aura(minor)</label>
        <description>This ability will weaken the stats of everyone around this super for the next 12 hours, after which the super will lie comatose for 12 hours. While Super Durable pawns will barely feel anything, this will make supers unsuper, and make unsupers twice as weak.</description>
        <iconPath>Base/UnsuperDurability</iconPath>
        <jobDef>CastAbilityOnThingWithoutWeapon</jobDef>
        <targetRequired>False</targetRequired>
        <cooldownTicksRange>60000</cooldownTicksRange>
        <verbProperties>
            <range>5</range>
            <requireLineOfSight>False</requireLineOfSight>
            <targetParams>
                <canTargetSelf>True</canTargetSelf>
            </targetParams>
        </verbProperties>
        <comps>
            <li Class = "CompProperties_AbilityGiveHediff">
                <compClass>CompAbilityEffect_GiveHediff</compClass>
                <hediffDef>SHG_AntiPower_Durability_MinorAura</hediffDef>
                <onlyBrain>False</onlyBrain>
            </li>
        </comps>
    </AbilityDef>

    <AbilityDef ParentName="SHGPowerAbilitiesBase">
        <defName>SHG_AntiPower_Durability_MajorAura</defName>
        <label>anti-durability aura(major)</label>
        <description>This ability will weaken the stats of everyone in a wide radius around this super for the next several hours, after which the super will lie comatose for 10 days. This will makebring Super Durability pawns down to a normal super, and will make everyone else unsupers at a minimum.</description>
        <iconPath>Base/AntisuperDurability</iconPath>
        <jobDef>CastAbilityOnThingWithoutWeapon</jobDef>
        <targetRequired>False</targetRequired>
        <cooldownTicksRange>60000</cooldownTicksRange>
        <verbProperties>
            <range>20</range>
            <requireLineOfSight>False</requireLineOfSight>
            <targetParams>
                <canTargetSelf>True</canTargetSelf>
            </targetParams>
        </verbProperties>
        <comps>
            <li Class = "CompProperties_AbilityGiveHediff">
                <compClass>CompAbilityEffect_GiveHediff</compClass>
                <hediffDef>SHG_AntiPower_Durability_MajorAura</hediffDef>
                <onlyBrain>False</onlyBrain>
            </li>
        </comps>
    </AbilityDef>

